<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>login</managed-bean-name>
        <managed-bean-class>com.guardian.Login.Login</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Login</from-outcome>
            <to-view-id>/Main.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Error</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>parmetertype</managed-bean-name>
        <managed-bean-class>com.guardian.Login.parmetertype</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>messages</managed-bean-name>
        <managed-bean-class>com.guardian.Login.Messages</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>inputtype</managed-bean-name>
        <managed-bean-class>com.guardian.Login.Inputtype</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>parameter</managed-bean-name>
        <managed-bean-class>com.guardian.Login.parameter</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
       <managed-bean>
        <managed-bean-name>Cinputtype</managed-bean-name>
        <managed-bean-class>com.guardian.converter.Cinputtype</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>cparmetertype</managed-bean-name>
        <managed-bean-class>com.guardian.converter.Cparmetertype</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
      <managed-bean>
        <managed-bean-name>CParameter</managed-bean-name>
        <managed-bean-class>com.guardian.converter.CParameter</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>parameterGroup</managed-bean-name>
        <managed-bean-class>com.guardian.Login.ParameterGroups</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>pGroup_tree</managed-bean-name>
        <managed-bean-class>com.guardian.Login.PGroup_tree</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>cgroupparameter</managed-bean-name>
        <managed-bean-class>com.guardian.converter.Cgroupparameter</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>app</managed-bean-name>
        <managed-bean-class>com.guardian.Login.application</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>
